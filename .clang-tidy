---
Checks:          '*, -google-runtime-references, -modernize-use-trailing-return-type, -modernize-use-transparent-functors'
WarningsAsErrors:  '*'
HeaderFilterRegex: ''
CheckOptions:
  - { key: readability-identifier-naming.CheckFunctionName, value: 'true' }
  - { key: readability-identifier-naming.FunctionNameCase, value: 'snake_case' }
  - { key: readability-identifier-naming.CheckVariable, value: 'true' }
  - { key: readability-identifier-naming.VariableCase, value: 'snake_case' }
  - { key: readability-identifier-naming.CheckClass, value: 'true' }
  - { key: readability-identifier-naming.ClassCase, value: 'PascalCase' }
  - { key: readability-identifier-naming.CheckStruct, value: 'true' }
  - { key: readability-identifier-naming.StructCase, value: 'PascalCase' }
  - { key: readability-identifier-naming.CheckEnumConstant, value: 'true' }
  - { key: readability-identifier-naming.EnumConstantCase, value: 'PascalCase' }
  - { key: readability-identifier-naming.CheckMemberVariable, value: 'true' }
  - { key: readability-identifier-naming.MemberVariableCase, value: 'snake_case' }
...